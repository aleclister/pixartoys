<div class="container new_camera" >
  <h1> Edit a new movie </h1>
  <%= simple_form_for([@movie]) do |f| %>
    <%= f.input :name, collection: @names, include_blank: false %>
    <%= f.input :description %>
    <%= f.input :release_date, start_year: Date.today.year - 40, end_year: Date.today.year %>
    <%= f.input :rating %>
    <%= f.input :photo, as: :attachinary %>
    <%= f.button :submit, class: "btn btn-danger" %>
  <% end %>
</div>

