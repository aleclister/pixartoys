<div class="container new_camera" >
  <h1> Add a new movie </h1>
  <%= simple_form_for([@user, @movie]) do |f| %>
    <%= f.input :name, collection: @names, include_blank: false %>
    <%= f.input :description %>
    <%= f.input :release_date %>
    <%= f.input :rating %>
    <%= f.input :photo, as: :attachinary %>
    <%= f.button :submit, class: "btn btn-danger" %>
  <% end %>
</div>

